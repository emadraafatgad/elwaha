<odoo>
    <data>

         <record id="oeh_clinic_diagnosis_action" model="ir.actions.act_window">
            <field name="name">Diagnosis</field>
            <field name="res_model">clinic.diagnosis</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form,search</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>


        <record model="ir.ui.view" id="oeh_clinic_diagnosis_form">
            <field name="name">model_clinic_diagnosis form</field>
            <field name="model">clinic.diagnosis</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group colspan="4">
                            <group col="2">
                                <field name="name" />
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="oeh_clinic_diagnosis_tree">
            <field name="name">model_clinic_diagnosis tree</field>
            <field name="model">clinic.diagnosis</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Diagnosis" >
                    <field name="name" />
                </tree>
            </field>
        </record>

        <record id="view_clinic_diagnosis_filter" model="ir.ui.view">
            <field name="name">model_clinic_diagnosis.search</field>
            <field name="model">clinic.diagnosis</field>
            <field name="priority">15</field>
            <field name="arch" type="xml">
                <search string="Search Diagnosis">
                    <field name="name" string="Name"/>
                </search>
            </field>
        </record>

        <menuitem id="oeh_medical_Diagnosis_menu"  name = "Diagnosis" parent="oeh_medical_config_menu" action="oeh_clinic_diagnosis_action" sequence="5"/>

        <record id="appointment_diagnosis_tree_id" model="ir.ui.view">
            <field name="name">Clinic Diagnosis</field>
            <field name="model">oeh.medical.appointment</field>
            <field name="inherit_id" ref="oeh_medical_appointment_view"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="inside">
                    <page name="diagnosis" string="Diagnosis">
                        <field name="diagnosis_ids" >
                            <tree string="Diagnosis" editable="bottom">
                                <field name="name"/>
                            </tree>
                        </field>
                    </page>
                </xpath>
            </field>
        </record>

    </data>
</odoo>