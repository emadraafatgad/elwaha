<odoo>
    <data>

         <record id="oeh_appointment_stages_action" model="ir.actions.act_window">
            <field name="name">Appointment Stages</field>
            <field name="res_model">appointment.stage</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form,search</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>


        <record model="ir.ui.view" id="oeh_appointment_stages_form">
            <field name="name">Appointment Stages</field>
            <field name="model">appointment.stage</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group colspan="4">
                            <group col="2">
                                <field name="name" />
                                <field name="fold" />
                                <field name="sequence"/>
                                <field name="state" string="State"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="oeh_appointment_stage_tree">
            <field name="name">Appointment Stages</field>
            <field name="model">appointment.stage</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Stages" >
                    <field name="name" />
                    <field name="fold" />
                    <field name="sequence"/>
                    <field name="state" string="state"/>
                </tree>
            </field>
        </record>

        <record id="view_appointment_stage_filter" model="ir.ui.view">
            <field name="name">appointment.stage.search</field>
            <field name="model">appointment.stage</field>
            <field name="priority">15</field>
            <field name="arch" type="xml">
                <search string="Search Opportunities">
                    <field name="name" string="Appointment"/>
                    <field name="state" string="state"/>
                </search>
            </field>
        </record>


         <record id="oeh_service_stages_action" model="ir.actions.act_window">
            <field name="name">Appointment Stages</field>
            <field name="res_model">service.stage</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form,search</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>


        <record model="ir.ui.view" id="oeh_service_stages_form">
            <field name="name">Service Stages</field>
            <field name="model">service.stage</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group colspan="4">
                            <group col="2">
                                <field name="name" />
                                <field name="fold" />
                                <field name="sequence"/>
                                <field name="state"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="oeh_service_stage_tree">
            <field name="name">Service Stages</field>
            <field name="model">service.stage</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Stages" >
                    <field name="name" />
                    <field name="fold" />
                    <field name="state"/>
                    <field name="sequence"/>
                </tree>
            </field>
        </record>

        <record id="view_service_stage_filter" model="ir.ui.view">
            <field name="name">service.stage.search</field>
            <field name="model">service.stage</field>
            <field name="priority">15</field>
            <field name="arch" type="xml">
                <search string="Search Session">
                    <field name="name" string="stage"/>
                    <field name="state" string="state"/>
                </search>
            </field>
        </record>
        <menuitem id="oeh_medical_stages_menu"  name = "Stages" parent="oeh_medical_config_menu" sequence="5"/>
        <menuitem id="oeh_appointment_stages_line_menu"  action="oeh_appointment_stages_action" name = "appointment stages" parent="oeh_medical_stages_menu" sequence="5"/>
        <menuitem id="oeh_serviice_stages_line_menu"  action="oeh_service_stages_action" name = "Sessions Stages" parent="oeh_medical_stages_menu" sequence="5"/>


    </data>
</odoo>