<odoo>
    <data>

         <record id="oeh_medical_service_machine_action_tree" model="ir.actions.act_window">
            <field name="name">Session List</field>
            <field name="res_model">oeh.medical.service.machine</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>

        <record model="ir.ui.view" id="oeh_medical_service_machine_tree">
            <field name="name">Sessions List</field>
            <field name="model">oeh.medical.service.machine</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Machines" editable="top">
                    <field name="machine_id" />
                    <field name="service_id" />
                    <field name="time_in_min"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="oeh_medical_service_machine_form">
            <field name="name">Sessions List</field>
            <field name="model">oeh.medical.service.machine</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group colspan="4">
                            <group col="2">
                                <field name="machine_id" />
                                <field name="service_id" />
                                <field name="time_in_min"/>
                            </group>
                        </group>

                    </sheet>
                </form>
            </field>
        </record>

        <menuitem id="oeh_service_machine_menu"  name = "Equipment" parent="oeh_medical_service_all_menu" sequence="5"/>
        <menuitem id="oeh_service_machine_line_menu"  action="oeh_medical_service_machine_action_tree" name = "Equipments" parent="oeh_service_machine_menu" sequence="5"/>
    </data>
</odoo>